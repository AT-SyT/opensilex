{
    "name": "opensilex-front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "check:outdated": "yarn outdated || cd .",
        "build:types": "dts-generator --project ./ --out ./types/opensilex-front.d.ts --prefix opensilex-front"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.25",
        "@fortawesome/free-solid-svg-icons": "^5.11.2",
        "@fortawesome/vue-fontawesome": "^0.1.8",
        "@kazupon/vue-i18n-loader": "^0.4.1",
        "bootstrap": "^4.4.1",
        "bootstrap-vue": "^2.1.0",
        "css-loader": "^3.4.0",
        "d3": "^5.14.2",
        "icon-kit": "^1.0.0",
        "jwt-decode": "^2.2.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.2",
        "vue-cookies": "^1.5.13",
        "vue-i18n": "^8.15.2",
        "vuejs-logger": "^1.5.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.1.0",
        "@vue/cli-plugin-eslint": "^4.1.0",
        "@vue/cli-plugin-router": "^4.1.0",
        "@vue/cli-plugin-typescript": "^4.1.0",
        "@vue/cli-plugin-vuex": "^4.1.0",
        "@vue/cli-service": "^4.1.0",
        "@vue/eslint-config-typescript": "^5.0.1",
        "chokidar": "^3.3.0",
        "simple-progress-webpack-plugin": "^1.1.2",
        "yargs-parser": "^16.1.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/typescript"
        ],
        "rules": {
            "no-unused-vars": "off"
        },
        "parserOptions": {
            "parser": "@typescript-eslint/parser"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
